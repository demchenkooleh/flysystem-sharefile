{
    "name": "demchenkooleh/flysystem-sharefile",
    "description": "Flysystem Adapter for ShareFile",
    "keywords": [
        "flysystem-sharefile",
        "flysystem",
        "sharfile",
        "api"
    ],
    "homepage": "https://github.com/demchenkooleh/flysystem-sharefile",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "ext-curl": "*",
        "demchenkooleh/sharefile-api": "dev-master",
        "league/flysystem": "^1.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.16",
        "larapack/dd": "^1.1",
        "mikey179/vfsstream": "^1.6",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5",
        "sabre/dav": "^3.2",
        "dms/phpunit-arraysubset-asserts": "^0.3.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:demchenkooleh/sharefile-api.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Citrix\\FlysystemSharefile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Citrix\\FlysystemSharefile\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
